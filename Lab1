            "text/html": [
              "\n",
              "     <input type=\"file\" id=\"files-34e741df-c6f8-4d78-a705-7be021c51c57\" name=\"files[]\" multiple disabled\n",
              "        style=\"border:none\" />\n",
              "     <output id=\"result-34e741df-c6f8-4d78-a705-7be021c51c57\">\n",
